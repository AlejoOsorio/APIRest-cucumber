{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Professor",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "identification": {
      "type": "string"
    },
    "phoneNumber": {
      "type": "string"
    },
    "roles": {
      "type": ["string", "null"]
    }
  },
  "required": [
    "id",
    "name",
    "lastName",
    "email",
    "identification"
  ],
  "additionalProperties": false
}
